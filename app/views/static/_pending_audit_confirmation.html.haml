- pending_audit_confirmations.each do |pending_confirmation|
  .clearfix
  = link_to "I confirm that I did not perform overtime for the week of: #{pending_confirmation.start_date}",
  audit_log_confirm_path(pending_confirmation), class: 'btn btn-primary', data: {confirm: 'Are you sure you did not do overtime?'}, id: "confirm_audit_log_#{pending_confirmation.id}"


